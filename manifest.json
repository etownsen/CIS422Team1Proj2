{
  "manifest_version": 2,

  "name": "EvalPal",
  "description": "This extension adds extra functionality to Duckweb",
  "version": "1.0",

  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}