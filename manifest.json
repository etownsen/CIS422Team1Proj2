{
  "manifest_version": 2,

  "name": "EvalPal",
  "description": "This extension adds extra functionality to Duckweb",
  "version": "1.0",

  "externally_connectable": {
    "matches": [
      "https://duckweb.uoregon.edu/*",
      "https://www.applyweb.com/*"
    ],
    "accepts_tls_channel_id": false
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "activeTab"
  ],
  "background":{
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}