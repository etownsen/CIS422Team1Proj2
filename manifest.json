{
	"manifest_version": 2,

	"name": "EvalPal",
	"description": "This extension adds extra functionality to Duckweb",
	"version": "1.0",
	"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

	"externally_connectable": {
		"matches": [
			"https://duckweb.uoregon.edu/*",
			"https://www.applyweb.com/*"
		],
		"accepts_tls_channel_id": false
	},
	"permissions": [
		"http://*/*",
		"https://*/*",
		"tabs",
		"activeTab"
	],
	"background":{
		"scripts": ["background.js"]
	},
	"page_action": {
		"default_icon": "icon48.png",
		"default_popup": "popup.html"
	}
}