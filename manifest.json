{
  "manifest_version": 2,

  "name": "Duckweb Plus",
  "description": "This extension adds extra functionality to Duckweb",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}